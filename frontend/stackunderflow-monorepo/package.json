{
  "name": "stackunderflow-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:desktop": "cd apps/desktop && npm run electron:dev",
    "dev:mobile": "cd apps/mobile && npm start"
  }
}
